func main() {
	// math functions
	let a: uint64 = uadd(3, 4);
	let b: uint64 = usub(a, 1);
	let c: uint64 = umul(a, b);
	let d: uint64 = udiv(c, usub(15, 12)); // Nested function calls are also supported

	// printing integars using `print_int`
	u64print(a);
	u64print(b);
	u64print(c);
	u64print(d);

	return 0;
}

