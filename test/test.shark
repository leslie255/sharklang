func pwr() {
	mov	rax, 1
	mov	r8, 0

	_loop_start:

	mul	rdi
	inc	r8

	cmp	r8, rsi
	je	_loop_end
	jmp	_loop_start
	_loop_end:
	
	return;
}

func main() {

	"hello, world";

	let a = 2;
	print_int(pwr(a, 1));
	print_int(pwr(a, 2));
	print_int(pwr(a, 3));
	print_int(pwr(a, 4));
	print_int(pwr(a, 5));

	return 0;
}

