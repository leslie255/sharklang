#import cstd.hhh

square: (x: usize) -> usize = {

    #asm
        mov     rax, rdi
        mul     rdi
    #end

    return _;
}

main: -> u32 = {
    a: = 16;
    x: = square(a ~ usize);
    printf("x = %lu\ta = %d\n", x, a ~ usize);
    if a == 16 {
        "lol\n";
    } else {
        "lmao\n";
    }
    return 0;
}
