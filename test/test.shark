func square(num: uint64) -> uint64 {
    // write assembly directly among higher level code!

    mov     rax, [rbp-8]
    mul     rax
    mov     [rbp - 8], rax

    return num;
}

func main() -> int32 {
    "hello, world";	// this implicitly calls the `println` function

    let msg = "the square of 16 is...";	// has type infer
    let a: uint64 = square(16);			// you can also specify a type ofc
    print(msg);
    u64print(a);

    return 0;
}
